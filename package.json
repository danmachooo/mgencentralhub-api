{
	"name": "mgencentralhub-api",
	"version": "1.0.0",
	"main": "index.js",
	"scripts": {
		"build": "tsc -p tsconfig.json && tsc-alias -p tsconfig.json",
		"dev": "tsx src/server.ts",
		"dev:watch": "tsx --watch src/server.ts",
		"db:reset": "tsx prisma/clear-storage.ts && npx prisma migrate reset",
		"db:reset:db-only": "npx prisma migrate reset",
		"format": "prettier . --write",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"postinstall": "npx prisma generate",
		"start": "node dist/server.js",
		"typecheck": "tsc -p tsconfig.json",
		"test": "echo \"No tests yet.\"",
		"test:s3": "tsx src/lib/s3.test.ts"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"description": "",
	"dependencies": {
		"@aws-sdk/client-s3": "^3.997.0",
		"@aws-sdk/s3-request-presigner": "^3.997.0",
		"@prisma/adapter-pg": "^7.4.0",
		"@prisma/client": "^7.4.1",
		"axios": "^1.13.2",
		"better-auth": "^1.4.18",
		"cors": "^2.8.5",
		"dotenv": "^17.3.1",
		"express": "^5.2.1",
		"multer": "^2.0.2",
		"pg": "^8.18.0",
		"tsc-alias": "^1.8.16",
		"winston": "^3.19.0",
		"zod": "^4.3.5"
	},
	"devDependencies": {
		"@eslint/js": "^9.39.2",
		"@types/cors": "^2.8.19",
		"@types/express": "^5.0.6",
		"@types/multer": "^2.0.0",
		"@types/node": "^25.2.3",
		"@types/pg": "^8.16.0",
		"eslint": "^9.39.2",
		"prettier": "^3.8.1",
		"prisma": "^7.4.1",
		"tsx": "^4.21.0",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.55.0"
	}
}
